  <!-- Guard render: chỉ may-cat* & luoi-cat*, gom theo data-sub -->
  <script>
    const ALLOW = /^(may-cat|luoi-cat)(-|$)/; // chặn lẫn danh mục
    const SUBS = ['may-cat-gach-ban','may-cat-gach-cam-tay','may-cat-da-nang','may-cat-sat-nhom','luoi-cat'];

    // Heuristic: route item -> sub dựa trên sku/name (không cần sửa affiliates.json)
    function routeSub(p){
      const s = deaccent((p.sku || p.name || '').toLowerCase());
      if (/gach.*ban|ban.*gach|gach-ban/.test(s)) return 'may-cat-gach-ban';
      if (/da-?nang|oscillat|rung|mcr/.test(s)) return 'may-cat-da-nang';
      if (/sat|nhom|cutoff|cut-off|mcs\d*|mcs/.test(s)) return 'may-cat-sat-nhom';
      if (/luoi|blade|da-cat|da-cat|dis[c|k]/.test(s)) return 'luoi-cat';
      // cầm tay/đá mài cắt → nhóm cầm tay
      if (/cam-?tay|cat-?goc|ag|mai-?cat|hg2|hkg2|6000j|50550/.test(s)) return 'may-cat-gach-cam-tay';
      return 'may-cat-gach-cam-tay';
    }
    function deaccent(str){ return str.normalize('NFD').replace(/[\u0300-\u036f]/g,''); }

    // Render 1 card (dùng origin, affiliate script sẽ rewrite nếu có)
    function cardHTML(p){
      const price = Number(p.price || 0);
      const priceText = price ? price.toLocaleString('vi-VN') + '₫' : 'Liên hệ';
      const url = p.origin || p.deeplink || '#';
      const name = escapeHTML(p.name || '');
      const img = p.image || '/assets/img/placeholder.webp';
      return `
      <article class="product-card" data-sku="${escapeAttr(p.sku||'')}" data-cat="${escapeAttr(p.category||'')}">
        <a class="thumb" href="${url}" target="_blank" rel="nofollow noopener" data-origin="${url}">
          <img loading="lazy" src="${img}" alt="${name}">
        </a>
        <div class="body">
          <h3 title="${name}"><a href="${url}" target="_blank" rel="nofollow noopener" data-origin="${url}">${name}</a></h3>
          <div class="price">${priceText}</div>
          <div class="actions">
            <a class="btn" href="${url}" target="_blank" rel="nofollow noopener" data-origin="${url}">Mua trên Shopee</a>
          </div>
        </div>
      </article>`;
    }
    function escapeHTML(s){return String(s).replace(/[&<>"']/g,m=>({ '&':'&amp;','<':'&lt;','>':'&gt;','"':'&quot;', "'":'&#39;' }[m]))}
    function escapeAttr(s){return String(s).replace(/"/g,'&quot;')}

    async function renderMayCat(){
      try{
        const res = await fetch('/assets/data/affiliates.json',{cache:'no-store'});
        const all = await res.json();

        // Lọc danh mục hợp lệ
        const valid = all.filter(p => p && p.status !== false && p.category && ALLOW.test(p.category));

        // Khử trùng lặp theo sku
        const seen = new Set();
        const list = valid.filter(p => !seen.has(p.sku) && seen.add(p.sku));

        // Gom nhóm theo sub
        const buckets = Object.fromEntries(SUBS.map(k => [k, []]));
        for (const p of list){
          const sub = routeSub(p);
          if (buckets[sub]) buckets[sub].push(p);
        }

        // Render theo từng grid
        document.querySelectorAll('.product-grid[data-category="may-cat"]').forEach(grid=>{
          const sub = grid.getAttribute('data-sub');
          const items = buckets[sub] || [];
          grid.innerHTML = items.map(cardHTML).join('');
        });

        // Fallback: nếu bucket trống, đừng để lỗ chỗ
        for (const sub of SUBS){
          const grid = document.querySelector(`.product-grid[data-sub="${sub}"]`);
          if (grid && !grid.children.length){
            const pool = list.slice(0,8);
            grid.innerHTML = pool.map(cardHTML).join('');
          }
        }

        // Kích hoạt affiliate rewrite nếu có
        if (window.MXD && typeof MXD.affRewrite === 'function'){ MXD.affRewrite(); }
      }catch(e){
        console.error('[may-cat] render error:', e);
        // Nếu lỗi fetch, dùng renderer cũ làm dự phòng
        if (window.MXD?.renderCategory) MXD.renderCategory('may-cat');
        else if (window.MXD?.renderProducts) MXD.renderProducts('.product-grid');
      }
    }

    window.addEventListener('DOMContentLoaded', renderMayCat);
  </script>
</body>
</html>

